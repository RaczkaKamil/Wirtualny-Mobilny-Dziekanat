<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/list_layout"
   >

    <TextView
        android:id="@+id/tf_przedmiot"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:text="Podstawy programowania obiektowego- w"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider3" />

    <TextView
        android:id="@+id/tf_t1"
        android:layout_width="18dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:text="3.5"
        android:textSize="12dp"
        app:layout_constraintBottom_toBottomOf="@+id/tf_przedmiot"
        app:layout_constraintEnd_toStartOf="@+id/divider5"
        app:layout_constraintTop_toTopOf="@+id/tf_przedmiot" />

    <TextView
        android:id="@+id/tf_t2"
        android:layout_width="18dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:text="2.5"
        android:textSize="12dp"
        app:layout_constraintEnd_toStartOf="@+id/divider6"
        app:layout_constraintTop_toTopOf="@+id/tf_t1" />

    <TextView
        android:id="@+id/tf_t3"
        android:layout_width="18dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:text="3.5"
        android:textSize="12dp"
        app:layout_constraintEnd_toStartOf="@+id/divider9"
        app:layout_constraintTop_toTopOf="@+id/tf_t2" />

    <TextView
        android:id="@+id/tf_aktywnosc"
        android:layout_width="18dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:text="5"
        android:textSize="12dp"
        app:layout_constraintEnd_toStartOf="@+id/divider11"
        app:layout_constraintTop_toTopOf="@+id/tf_t3" />

    <View
        android:id="@+id/divider2"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_marginTop="8dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tf_przedmiot" />

    <View
        android:id="@+id/divider3"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/divider4"
        android:layout_width="1dp"
        android:layout_height="0dp"
        android:layout_marginEnd="8dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintBottom_toBottomOf="@+id/divider2"
        app:layout_constraintEnd_toStartOf="@+id/tf_t1"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/divider5"
        android:layout_width="1dp"
        android:layout_height="0dp"
        android:layout_marginEnd="8dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintBottom_toBottomOf="@+id/divider2"
        app:layout_constraintEnd_toStartOf="@+id/tf_t2"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/divider6"
        android:layout_width="1dp"
        android:layout_height="0dp"
        android:layout_marginEnd="8dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintBottom_toBottomOf="@+id/divider2"
        app:layout_constraintEnd_toStartOf="@+id/tf_t3"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.51" />

    <View
        android:id="@+id/divider9"
        android:layout_width="1dp"
        android:layout_height="0dp"
        android:layout_marginEnd="8dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintBottom_toBottomOf="@+id/divider2"
        app:layout_constraintEnd_toStartOf="@+id/tf_aktywnosc"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/divider11"
        android:layout_width="1dp"
        android:layout_height="0dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintBottom_toBottomOf="@+id/divider2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>